<script src="https://maps.google.com/maps/api/js?sensor=false&.js"></script>

<script>
  var materialsId  = <%= raw t('materials_id').to_json %>;
  var currentMaterial = '<%= @material_key %>';
</script>


<div class="material-list">
  <div class="container">
    <%= material_links %>
  </div>
</div>

<div class="results clear">
  <div class="container">
    <span class="results-total"><%= @material %> - <span data-total-results>0</span> resultados</span>
  </div>
</div>

<div class="clear"></div>

<div class="map-content">
  <div class="container">
    <div class="cooperatives-list">
      <strong class="title">Cooperativas</strong>
      <ul data-cooperatives-target>
        <%= image_tag 'load.gif' %>
      </ul>
    </div>

    <div class="map">
      <strong class="map-target title">Mapa</strong>
      <div class='gmap' data-map-target>
        <%= image_tag 'load.gif' %>
      </div>
    </div>
  </div>
</div>

<script type="text/template" data-cooperative-template>
   <li data-cooperative-item data-cooperative-index="{{index}}" data-cooperative-{{lat}}>
     <strong>{{name}}</strong>
     <address>{{location}}</address>
   </li>
</script>
